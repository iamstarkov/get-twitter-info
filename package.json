  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamstarkov/get-twitter-info/issues"
  },
  "homepage": "https://github.com/iamstarkov/get-twitter-info#readme",
  "devDependencies": {
    "assert": "^1.3.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "coveralls": "*",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "isparta": "^4.0.0",
    "mocha": "*",
    "npm-run-all": "*",
    "rimraf": "*",
    "twitter-tokens": "^1.0.2"
  },
